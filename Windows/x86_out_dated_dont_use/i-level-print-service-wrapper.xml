<service>
    <name>i Level Print Service</name>
    <!-- ID of the service. It should be unique across the Windows system-->
    <id>iLevelPrintService</id>

    <!-- Path to the executable, which should be started -->
    <!-- CAUTION: Don't put arguments here. Use <arguments> instead. -->
    <executable>%BASE%\..\..\i-level-print-service\i-level-print-service-win.exe</executable>
    <description>This the service for the print server connected to i Level</description>
</service>