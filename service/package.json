{
  "name": "web-print-service",
  "version": "1.0.0",
  "description": "Print service application",
  "main": "./src/server.js",
  "bin": "./src/server.js",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/server.js",
    "build": "pkg . --compress=GZip"
  },
  "author": "Samuel Guard, Joseph Cryer",
  "license": "ISC",
  "dependencies": {
    "@thiagoelg/node-printer": "^0.5.8",
    "axios": "^0.21.4",
    "es-get-iterator": "github:jcryer/es-get-iterator",
    "express": "^4.18.1",
    "jimp": "^0.16.1",
    "jsbarcode": "^3.11.5",
    "pdfkit": "^0.13.0",
    "svg-to-pdfkit": "^0.1.8",
    "unix-print": "^1.1.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "pkg": "^5.7.0"
  },
  "pkg": {
    "assets": [
      "static/",
      "/node_modules/pdfkit/js/data/"
    ],
    "scripts": [
      "src/**/*.js",
      "node_modules/@thiagoelg/node-printer/lib/node_printer.node"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-arm64"
    ],
    "outputPath": "out/"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  }
}
